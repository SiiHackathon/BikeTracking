@model IEnumerable<BikeTracker.Models.NewsDetailsViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>View</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
@foreach (var item in Model)
{
    <tr>
        <th>
            @Html.DisplayFor(x => item.Title)
            @Html.DisplayFor(x => item.CreatedOn)
        </th>
    </tr>
    <tr>
        <td>
            @Html.Raw(item.Content)
        </td>
